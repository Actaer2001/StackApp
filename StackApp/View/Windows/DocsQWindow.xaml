<Window x:Class="StackApp.View.Windows.DocsQWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StackApp.View.Windows"
        xmlns:vm="clr-namespace:StackApp.ViewModel"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStyle="None"
        Title="DocsQWindow" Height="100" Width="200"
        WindowStartupLocation="CenterScreen">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>

        <Button Content="English?" Click="Button_Click"/>

        <Button Grid.Column="1" Content="Srpski?" Click="Button_Click_1"/>
    </Grid>
</Window>
